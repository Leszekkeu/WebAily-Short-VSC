{
	"name": "webaily",
	"displayName": "WebAily Short",
	"description": "The WebAily extension was created for faster shortening of URLS directly from VSC.",
	"version": "0.0.2",
	"repository": "https://github.com/leszekkorzan/WebAily-Short-VSC",
	"homepage": "https://webaily.web.app",
	"publisher": "LeszekKorzan",
	"engines": {
		"vscode": "^1.41.0"
	},
	"categories": [
		"Other"
	],
	"keywords": [
		"WebAily",
		"Short",
		"shortURL",
		"shortLink",
		"link",
		"API",
		"shortly",
		"url",
		"short url",
		"short link"
	],
	"activationEvents": [
		"onCommand:webaily.short"
	],
	"icon": "webaily-vsc-ext-logo.png",
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "webaily.short",
				"title": "Short Link"
			}
		],
		"menus": {
			"editor/context": [
			  {
				"command": "webaily.short",
				"group": "WebAily@1"
			  }
			]
		}
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.1",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.41.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^7.0.1",
		"typescript": "^3.7.5",
		"vscode-test": "^1.3.0"
	},
	"dependencies": {
		"unirest": "^0.6.0"
	}
}
